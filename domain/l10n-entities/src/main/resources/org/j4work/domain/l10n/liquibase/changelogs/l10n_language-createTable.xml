<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="l10n_language-createTable" author="j4work">
        <!--
        Create l10n_language table with default name
        -->
        <createTable tableName="l10n_language">
            <column name="id" type="CHAR(2)" autoIncrement="false">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="name" type="NVARCHAR(31)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>