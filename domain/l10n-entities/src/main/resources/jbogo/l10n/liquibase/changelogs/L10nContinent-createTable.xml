<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="L10nContinent-createTable" author="jbogo">
        <!--
        Create L10nContinent table with default name
        -->
        <createTable tableName="L10nContinent">
            <column name="id" type="CHAR(2)" autoIncrement="false">
                <constraints primaryKey="true" nullable="false" />
            </column>

            <column name="name" type="NVARCHAR(31)">
                <constraints nullable="false" />
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>